    "nlist = []\n",
    "for num1 in range(100, 999):\n",
    "    for num2 in range(100, 999):\n",
    "        result = num1 * num2\n",
    "        palindrome  = str(result) \n",
    "        n = 0\n",
    "        if len(palindrome)%2 == 0:\n",
    "            for i in range(len(palindrome)//2):\n",
    "                if palindrome[i] == palindrome[-(i+1)]:\n",
    "                    n+=1\n",
    "                else :\n",
    "                    n = 0\n",
    "                    break\n",
    "\n",
    "        if n == len(palindrome)/2:\n",
    "            nlist.append(palindrome)\n",
    "        \n",
    "print(max(nlist))"
   ]
